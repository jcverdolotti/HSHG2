<div>
<% if signed_in? %>
   <%= image_tag("hs.png",size: "120x80", class:"d-inline-block align-top")%>
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        </ol>
       
       
        <div class="carousel-inner">
          
            <div class="carousel-item active">
              <img class="d-block w-100" src="<%= Residence.find(@hs.first.residence_id).image %>"  alt="First slide">
              <div class="carousel-caption d-none d-md-block">
                <h1><%= link_to Residence.find(@hs.first.residence_id).name, hotsale_path(@hs.first) %></h1>
                <p><%= Residence.find(@hs.first.residence_id).description%></p>
                <p><%= Week.find(@hs.first.week_id).week_date %></p>
              </div>
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="<%= Residence.find(@hs.last.residence_id).image %>"  alt="Second slide">
              <div class="carousel-caption d-none d-md-block">
                <h1><%= link_to Residence.find(@hs.last.residence_id).name, hotsale_path(@hs.last) %></h1>
                <p><%= Residence.find(@hs.last.residence_id).description%></p>
                <p><%= Week.find(@hs.last.week_id).week_date %></p>
              </div>
            </div> 
        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Anterior</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Siguiente</span>
        </a>
    </div>
